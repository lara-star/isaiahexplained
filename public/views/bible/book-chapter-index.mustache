<main class="col-xs-12 col-sm-8 left-column" style="padding-top: 0;" role="main">
    <h1 class="title-chapters"><a href="{{app_url}}/bible/"><i class="fa fa-book"></i></a><i class="fa fa-angle-right"></i>{{book_title}}</h1>
    <hr>
    <div class="chapter-table one-col-padding">
        <ul class="books">
            {{#book_chapters}}
                <li>
                    <a href="{{app_url}}/bible/{{book_abbr}}/{{chapter_number}}">{{chapter_number}}</a>
                </li>
            {{/book_chapters}}
        </ul>
    </div>
</main>
